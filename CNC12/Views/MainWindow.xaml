<Window x:Class="CNC12.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes" 
        xmlns:uc="clr-namespace:CNC12.UserControls"
        xmlns:cv="clr-namespace:CNC12.ConvertValue"
        xmlns:lg="clr-namespace:CNC12.ViewModels"
        TextElement.Foreground="{DynamicResource MaterialDesignBody}"
        TextElement.FontWeight="Regular"
        TextElement.FontSize="13"
        TextOptions.TextFormattingMode="Ideal"
        TextOptions.TextRenderingMode="Auto"
        Background="White"
        FontFamily="{DynamicResource MaterialDesignFont}"
        DataContext="{StaticResource VMmainWindow}"
        ResizeMode="CanResizeWithGrip"
        WindowStyle="None"
        Title="MAIN WINDOW"
        mc:Ignorable="d"
        Name="MainWindow1"
        Height="800" Width="1630">
    <i:Interaction.Triggers>
        <i:EventTrigger EventName="Loaded">
            <i:InvokeCommandAction                    
                Command="{Binding LoadedWindowCommand}"                  
                CommandParameter="{Binding ElementName=MainWindow1}"/>
        </i:EventTrigger>
        <i:EventTrigger EventName="Closing">
            <i:InvokeCommandAction                    
                Command="{Binding ClosedWindowCommand}"                  
                CommandParameter="{Binding ElementName=MainWindow1}"/>
        </i:EventTrigger>
    </i:Interaction.Triggers>
    <Window.Resources>
        <lg:VMmanagerWindow x:Key="user"></lg:VMmanagerWindow>
        <cv:ColorConvertValue x:Key="ColorConvertValuene"/>
    </Window.Resources>

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"></RowDefinition>
            <RowDefinition Height="80"></RowDefinition>
            <RowDefinition Height="80"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>

        <Grid Grid.Row="0">
            <uc:ControlBar Tag="{Binding Title, ElementName=MainWindow1}"></uc:ControlBar>
        </Grid>

        <Grid Grid.Row="1" 
              HorizontalAlignment="Stretch" 
              VerticalAlignment="Top">
            <Image x:Name="image" Visibility="Visible"
                   HorizontalAlignment="Stretch" 
                   Height="83"  
                   VerticalAlignment="Top" 
                   Width="1400"  
                   Source="/Images/lgo6.PNG" 
                   Margin="115,0,115,-13"  />
        </Grid>

        <materialDesign:Card Grid.Row="2"           
                Foreground="{DynamicResource PrimaryHueDarkForegroundBrush}"               
                HorizontalAlignment="Stretch"               
                VerticalAlignment="Stretch"   
                Margin="5 0 5 8"           
                UniformCornerRadius="10" Grid.ColumnSpan="2">
            <materialDesign:Card.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0"/>
                    <GradientStop Color="#FFEDB2B2" Offset="0.758"/>
                </LinearGradientBrush>
            </materialDesign:Card.Background>
        </materialDesign:Card>
        <Grid Grid.Row="2" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="200"></ColumnDefinition>
                <ColumnDefinition Width="90"></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition ></ColumnDefinition>
                <ColumnDefinition Width="130"></ColumnDefinition>

            </Grid.ColumnDefinitions>
            <materialDesign:Card    
                    Grid.Column="0"
                    UniformCornerRadius="10"
                    Background="{DynamicResource PrimaryHueLightBrush}"      
                    Foreground="{DynamicResource PrimaryHueLightForegroundBrush}"                   
                    Margin="15 8 0 13">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="auto"></ColumnDefinition>
                        <ColumnDefinition Width="auto"></ColumnDefinition>

                    </Grid.ColumnDefinitions>
                    <Grid Grid.Column="0">
                        <materialDesign:PackIcon 
                            Margin="10 0 0 0"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            Width="30" Height="30"
                            Kind="AccountCircle" />
                    </Grid>
                    <TextBlock
                        x:Name="Usertext"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center" 
                        Margin="5 0 0 0"
                        Text="{Binding UserName ,Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                        FontSize="26"
                        FontWeight="Bold"
                        Width="auto"
                        Grid.Column="1">
                        
                    </TextBlock>
                </Grid>
            </materialDesign:Card>
            <Button ToolTip="Report"
                        Grid.Column="2"
                        Command="{Binding ReportCommand}"
                        VerticalAlignment="Stretch"
                        HorizontalAlignment="Stretch"
                        Width="auto"
                        Height="auto"
                        Margin="20 8 20 13"                
                        Style="{DynamicResource MaterialDesignRaisedButton}"   
                        materialDesign:ShadowAssist.ShadowDepth="Depth3">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Grid Grid.Row="0">
                        <TextBlock 
                            Text="REPORT" 
                            FontSize="20"
                            HorizontalAlignment="Center"     
                            VerticalAlignment="Center" />
                    </Grid>
                    <Grid Grid.Row="1">
                        <materialDesign:PackIcon                                 
                            Kind="BookOpen"    
                            Width="20" Height="20"                                
                            HorizontalAlignment="Center"    
                            VerticalAlignment="Center"/>
                    </Grid>
                </Grid>
            </Button>
            <Button ToolTip="Chart"
                    Grid.Column="3"
                    Command="{Binding ChartCommand}"       
                    VerticalAlignment="Stretch"
                        HorizontalAlignment="Stretch"
                        Width="auto"
                        Height="auto"
                        Margin="20 8 20 13"                 
                        Style="{DynamicResource MaterialDesignRaisedButton}"   
                        materialDesign:ShadowAssist.ShadowDepth="Depth3">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Grid Grid.Row="0">
                        <TextBlock 
                            Text="CHART" 
                            FontSize="20"
                            HorizontalAlignment="Center"     
                            VerticalAlignment="Center" />
                    </Grid>
                    <Grid Grid.Row="1">
                        <materialDesign:PackIcon                                 
                            Kind="ChartBar"    
                            Width="20" Height="20"                                
                            HorizontalAlignment="Center"    
                            VerticalAlignment="Center"/>
                    </Grid>
                </Grid>
            </Button>

            <Button ToolTip="Warning"
                    Grid.Column="4"
                    Command="{Binding WarningCommand}"       
                    VerticalAlignment="Stretch"
                        HorizontalAlignment="Stretch"
                        Width="auto"
                        Height="auto"
                        Margin="20 8 20 13"                   
                        Style="{DynamicResource MaterialDesignRaisedButton}"   
                        materialDesign:ShadowAssist.ShadowDepth="Depth3">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Grid Grid.Row="0">
                        <TextBlock 
                            Text="WARNING" 
                            FontSize="20"
                            HorizontalAlignment="Center"     
                            VerticalAlignment="Center" />
                    </Grid>
                    <Grid Grid.Row="1">
                        <materialDesign:PackIcon                                 
                            Kind="Alert"    
                            Width="20" Height="20"                                
                            HorizontalAlignment="Center"    
                            VerticalAlignment="Center"/>
                    </Grid>
                </Grid>
            </Button>
            <!--IsEnabled="{Binding boolSetting, UpdateSourceTrigger=PropertyChanged}"-->
            <Button x:Name="SettingButton"
                    ToolTip="Setting"
                    Grid.Column="6"
                    IsEnabled="{Binding boolSetting, UpdateSourceTrigger=PropertyChanged}"
                    Command="{Binding SettingCommand}"       
                    VerticalAlignment="Stretch"
                        HorizontalAlignment="Stretch"
                        Width="auto"
                        Height="auto"
                        Margin="20 8 20 13"                  
                        Style="{DynamicResource MaterialDesignRaisedButton}"   
                        materialDesign:ShadowAssist.ShadowDepth="Depth3">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Grid Grid.Row="0">
                        <TextBlock 
                            Text="SETTING" 
                            FontSize="20"
                            HorizontalAlignment="Center"     
                            VerticalAlignment="Center" />
                    </Grid>
                    <Grid Grid.Row="1">
                        <materialDesign:PackIcon                                 
                            Kind="Settings"    
                            Width="20" Height="20"                                
                            HorizontalAlignment="Center"    
                            VerticalAlignment="Center"/>
                    </Grid>
                </Grid>
            </Button>

            <Button ToolTip="Contact"
                    Grid.Column="5"
                    IsEnabled="{Binding boolSetting, UpdateSourceTrigger=PropertyChanged}"
                    Command="{Binding ContactCommand}"       
                    VerticalAlignment="Stretch"
                        HorizontalAlignment="Stretch"
                        Width="auto"
                        Height="auto"
                        Margin="20 8 20 13"                
                        Style="{DynamicResource MaterialDesignRaisedButton}"   
                        materialDesign:ShadowAssist.ShadowDepth="Depth3">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Grid Grid.Row="0">
                        <TextBlock 
                            Text="CONTACT" 
                            FontSize="20"
                            HorizontalAlignment="Center"     
                            VerticalAlignment="Center" />
                    </Grid>
                    <Grid Grid.Row="1">
                        <materialDesign:PackIcon                                 
                            Kind="PhoneReturn"    
                            Width="20" Height="20"                                
                            HorizontalAlignment="Center"    
                            VerticalAlignment="Center"/>
                    </Grid>
                </Grid>
            </Button>
            <materialDesign:Card  
                UniformCornerRadius="10"
                Margin="20,8,0,13"
                Grid.Column="1"                                 
                Foreground="{DynamicResource PrimaryHueLightForegroundBrush}">
                <materialDesign:Card.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#FF119A11" Offset="1"/>
                        <GradientStop Color="#FFE7F1E7"/>
                    </LinearGradientBrush>
                </materialDesign:Card.Background>
                <StackPanel Orientation="Horizontal" >
                    <materialDesign:PackIcon 
                        Grid.Column="0"
                            Margin="10 0 0 0"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            Width="30" Height="30"
                            Kind="CastConnected" />
                    <!--IsEnabled="{Binding boolSetting, UpdateSourceTrigger=PropertyChanged}"-->
                    <materialDesign:PopupBox      
                        
                        Width="auto" Height="auto"
                        StaysOpen="True"                          
                        FontWeight="Bold">
                        <Grid Width="300" Height="auto">
                            <Grid.RowDefinitions>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Button 
                                    IsEnabled="{Binding user1, UpdateSourceTrigger=PropertyChanged}"   
                                    VerticalAlignment="Stretch"
                                    HorizontalAlignment="Stretch"
                                    Width="200" 
                                    Height="60"      
                                    Margin="5 10 5 5"
                                    Style="{DynamicResource MaterialDesignRaisedButton}"   
                                    materialDesign:ShadowAssist.ShadowDepth="Depth3" Background="#FF46A000">
                                    <Grid VerticalAlignment="Stretch"
                                    HorizontalAlignment="Stretch">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="40"/>
                                            <ColumnDefinition Width="150"/>
                                        </Grid.ColumnDefinitions>
                                      
                                            <TextBlock Grid.Column="1"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Margin="0 0 30 0"
                                                Text="admin" 
                                                FontSize="25"
                                                 />     
                                            <materialDesign:PackIcon Grid.Column="0"                                
                                                Kind="AccountLocation"    
                                                Width="20" Height="20" 
                                                Margin="10 0 0 0"
                                                HorizontalAlignment="Center"     
                                                VerticalAlignment="Center"/>   
                                    </Grid>
                                </Button>
                            </Grid>
                            <Grid Grid.Row="1">
                                <Button 
                                    IsEnabled="{Binding user2, UpdateSourceTrigger=PropertyChanged}"   
                                     VerticalAlignment="Stretch"
                                    HorizontalAlignment="Stretch"
                                    Width="200" 
                                    Height="60"      
                                    Margin="5 10 5 5"
                                    Style="{DynamicResource MaterialDesignRaisedButton}"   
                                    materialDesign:ShadowAssist.ShadowDepth="Depth3" Background="#FF46A000">
                                    <Grid VerticalAlignment="Stretch"
                                    HorizontalAlignment="Stretch">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="40"/>
                                            <ColumnDefinition Width="150"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock Grid.Column="1"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Margin="0 0 30 0"
                                                Text="nhanvien1" 
                                                FontSize="25"
                                                 />
                                        <materialDesign:PackIcon Grid.Column="0"                                
                                                Kind="AccountLocation"    
                                                Width="20" Height="20" 
                                                Margin="10 0 0 0"
                                                HorizontalAlignment="Center"     
                                                VerticalAlignment="Center"/>
                                    </Grid>
                                </Button>
                            </Grid>
                            <Grid Grid.Row="2">
                                <Button 
                                    IsEnabled="{Binding user3, UpdateSourceTrigger=PropertyChanged}"   
                                    VerticalAlignment="Stretch"
                                    HorizontalAlignment="Stretch"
                                    Width="200" 
                                    Height="60"      
                                    Margin="5 10 5 5"
                                    Style="{DynamicResource MaterialDesignRaisedButton}"   
                                    materialDesign:ShadowAssist.ShadowDepth="Depth3" Background="#FF46A000">
                                    <Grid VerticalAlignment="Stretch"
                                    HorizontalAlignment="Stretch">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="40"/>
                                            <ColumnDefinition Width="150"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock Grid.Column="1"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Margin="0 0 30 0"
                                                Text="nhanvien2" 
                                                FontSize="25"
                                                 />
                                        <materialDesign:PackIcon Grid.Column="0"                                
                                                Kind="AccountLocation"    
                                                Width="20" Height="20" 
                                                Margin="10 0 0 0"
                                                HorizontalAlignment="Center"     
                                                VerticalAlignment="Center"/>
                                    </Grid>
                                </Button>
                            </Grid>
                            <Grid Grid.Row="3">
                                <Button 
                                    IsEnabled="{Binding user4, UpdateSourceTrigger=PropertyChanged}"   
                                    VerticalAlignment="Stretch"
                                    HorizontalAlignment="Stretch"
                                    Width="200" 
                                    Height="60"      
                                    Margin="5 10 5 5"
                                    Style="{DynamicResource MaterialDesignRaisedButton}"   
                                    materialDesign:ShadowAssist.ShadowDepth="Depth3" Background="#FF46A000">
                                    <Grid VerticalAlignment="Stretch"
                                    HorizontalAlignment="Stretch">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="40"/>
                                            <ColumnDefinition Width="150"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock Grid.Column="1"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                Margin="0 0 30 0"
                                                Text="nhanvien3" 
                                                FontSize="25"
                                                 />
                                        <materialDesign:PackIcon Grid.Column="0"                                
                                                Kind="AccountLocation"    
                                                Width="20" Height="20" 
                                                Margin="10 0 0 0"
                                                HorizontalAlignment="Center"     
                                                VerticalAlignment="Center"/>
                                    </Grid>
                                </Button>
                            </Grid>
                        </Grid>

                    </materialDesign:PopupBox>
                </StackPanel>
            </materialDesign:Card>
            <materialDesign:Card  
                UniformCornerRadius="5"
                Margin="0,8,13,13"
                Grid.Column="7"
                Background="{DynamicResource PrimaryHueLightBrush}"                                 
                Foreground="{DynamicResource PrimaryHueLightForegroundBrush}">
                <StackPanel 
                    Orientation="Horizontal"  >
                    <TextBlock        
                        FontFamily="arial"
                        Text="LINE"                        
                        Grid.Column="0"
                        FontSize="30"                  
                        Margin="15 12 0 0" FontWeight="Bold"  >
                        <TextBlock.Foreground>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="Black" Offset="0"/>
                                <GradientStop Color="#FF3E46A7" Offset="0.503"/>
                            </LinearGradientBrush>
                        </TextBlock.Foreground>
                    </TextBlock>
                    
                    <materialDesign:PopupBox   
                                    Width="40"
                                    HorizontalAlignment="Center"
                                    Margin="-3,0,0,0"                          
                                    StaysOpen="True"                          
                                    IsEnabled="True" FontWeight="Bold">
                        <materialDesign:PopupBox.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="Black" Offset="0"/>
                                <GradientStop Color="#FF74E276" Offset="0.581"/>
                            </LinearGradientBrush>
                        </materialDesign:PopupBox.Background>
                        <Grid                               
                                        Width="200"                          
                                        Margin="8,8,8,8">
                            <Grid.RowDefinitions>
                                <RowDefinition  Height="*" />
                                <RowDefinition  Height="*" />
                            </Grid.RowDefinitions>

                            <WrapPanel Grid.Row="0" Width="180">
                                <RadioButton 
                                                GroupName="PickLine"
                                                Height="50"
                                                Width="90"
                                                Content="Line 1"
                                                IsChecked="True"
                                                FontSize="20"
                                                Command="{Binding ChonMayCNC1Command}"
                                                Style="{StaticResource MaterialDesignRadioButton}"
                                                Tag="True">
                                </RadioButton>
                                <RadioButton 
                                                GroupName="PickLine"
                                                Height="50"
                                                Width="90"
                                                Content="Line 2"
                                                FontSize="20" 
                                                Command="{Binding ChonMayCNC2Command}"
                                                Style="{StaticResource MaterialDesignRadioButton}"
                                                Tag="True">
                                </RadioButton>
                            </WrapPanel>

                            <WrapPanel Grid.Row="1" Width="180">
                                <RadioButton 
                                                GroupName="PickLine"
                                                Height="50"
                                                Width="90"
                                                Content="Line 3"
                                                FontSize="20"
                                                Command="{Binding ChonMayCNC3Command}"
                                                Style="{StaticResource MaterialDesignRadioButton}"
                                                Tag="True">

                                </RadioButton>
                                <RadioButton 
                                                GroupName="PickLine"
                                                Height="50"
                                                Width="90"
                                                Content="Line 4"
                                                FontSize="20" 
                                                Command="{Binding ChonMayCNC4Command}"
                                                Style="{StaticResource MaterialDesignRadioButton}"
                                                Tag="True">
                                </RadioButton>
                            </WrapPanel>
                        </Grid>
                    </materialDesign:PopupBox>
                </StackPanel>
            </materialDesign:Card>
        </Grid>
        <materialDesign:Card
                Grid.Row="3"              
                Foreground="{DynamicResource PrimaryHueDarkForegroundBrush}"               
                HorizontalAlignment="Stretch"               
                VerticalAlignment="Stretch"   
                Margin="5 0 5 5"
                Padding="8"              
                UniformCornerRadius="10" Grid.ColumnSpan="2">
            <materialDesign:Card.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0.64"/>
                    <GradientStop Color="#FFFCFCFC" Offset="1"/>
                </LinearGradientBrush>
            </materialDesign:Card.Background>
            <ScrollViewer 
                HorizontalScrollBarVisibility="Auto"
                VerticalScrollBarVisibility="auto">
                <Grid >
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="200"></ColumnDefinition>
                        <ColumnDefinition Width="200"></ColumnDefinition>
                        <ColumnDefinition Width="200"></ColumnDefinition>
                        <ColumnDefinition Width="200"></ColumnDefinition>
                        <ColumnDefinition Width="200"></ColumnDefinition>
                        <ColumnDefinition Width="200"></ColumnDefinition>
                        <ColumnDefinition Width="200"></ColumnDefinition>
                        <ColumnDefinition Width="200"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Border Grid.Column="0" BorderThickness="5"
                        Background="#FFD7EFAA">
                        <Grid >
                            <Grid.RowDefinitions>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                            </Grid.RowDefinitions>
                            <materialDesign:Card
                                Grid.Row="0"
                                Margin="5 5 5 5"
                                Background="#FFAE9AD3"
                                Foreground="{DynamicResource PrimaryHueLightForegroundBrush}"
                                materialDesign:ShadowAssist.ShadowDepth="Depth5"
                                UniformCornerRadius="10">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid Grid.Row="0">
                                        <materialDesign:PackIcon 
                                                Kind="AccessPointNetwork" 
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center" 
                                                Width="40" Height="40"
                                                Margin="0 0 0 5"/>
                                    </Grid>
                                    <Grid Grid.Row="1">
                                        <TextBlock 
                                                FontSize="26"
                                                FontWeight="Bold"
                                                Text="{Binding Wise11, Mode=TwoWay}"
                                                HorizontalAlignment="Center"     
                                                VerticalAlignment="Center"  />

                                    </Grid>
                                </Grid>
                            </materialDesign:Card>

                            <materialDesign:Card
                                Grid.Row="1"
                                Margin="5 5 5 5"
                                Background="#FF9AC5D3"
                                Foreground="{DynamicResource PrimaryHueLightForegroundBrush}"
                                materialDesign:ShadowAssist.ShadowDepth="Depth5"
                                UniformCornerRadius="10">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid Grid.Row="0">
                                        <materialDesign:PackIcon 
                                                Kind="DoorOpen" 
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center" 
                                                Width="40" Height="40"
                                                Margin="0 0 0 5"/>
                                    </Grid>
                                    <Grid Grid.Row="1">
                                        <TextBlock 
                                                FontSize="26"
                                                FontWeight="Bold"
                                                Text="{Binding StatusDoor1, Mode=TwoWay}"
                                                HorizontalAlignment="Center"     
                                                VerticalAlignment="Center"  />
                                    </Grid>
                                </Grid>
                            </materialDesign:Card>

                            <materialDesign:Card
                                Grid.Row="2"
                                Margin="5 5 5 5"
                                Background="#FF9AC5D3"
                                Foreground="{DynamicResource PrimaryHueLightForegroundBrush}"
                                materialDesign:ShadowAssist.ShadowDepth="Depth5"
                                UniformCornerRadius="10">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid Grid.Row="0">
                                        <materialDesign:PackIcon 
                                                Kind="MoveResizeVariant" 
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center" 
                                                Width="40" Height="40"
                                                Margin="0 0 0 5"/>
                                    </Grid>
                                    <Grid Grid.Row="1">
                                        <TextBlock 
                                                FontSize="26"
                                                FontWeight="Bold"
                                                Text="{Binding StatusMachine1, Mode=TwoWay}"
                                                HorizontalAlignment="Center"     
                                                VerticalAlignment="Center"   />
                                    </Grid>
                                </Grid>
                            </materialDesign:Card>
                            <Viewbox 
                                Grid.Row="3"
                                HorizontalAlignment="Center" 
                                Height="100" 
                                VerticalAlignment="Center"                               
                                Width="100" RenderTransformOrigin="0.104,1.184">
                                <Viewbox>
                                    <Canvas Width="100" Height="100">
                                        <Ellipse Width="100"
                                                 Height="100"
                                                 Canvas.Left="0"
                                                 Canvas.Top="0"
                                                 Stroke="Gray"
                                                 StrokeThickness="0.5">
                                            <Ellipse.Fill>
                                                <LinearGradientBrush StartPoint="0.5,1" 
                                                             EndPoint="1,0"
                                                             SpreadMethod="Pad">
                                                    <GradientStop Color="White"
                                                          Offset="1"/>
                                                    <GradientStop Color="#FF071923"
                                                          Offset="0"/>
                                                </LinearGradientBrush>
                                            </Ellipse.Fill>
                                        </Ellipse>
                                        <Ellipse Width="85"
                                                 Height="85"
                                                 Canvas.Left="7.25"
                                                 Canvas.Top="7.25"
                                                 Stroke="Gray"
                                                 StrokeThickness="0.5">
                                            <Ellipse.Fill>
                                                <LinearGradientBrush StartPoint="1,0.5" 
                                                             EndPoint="0,1"
                                                             SpreadMethod="Pad">
                                                    <GradientStop Color="White"
                                                            Offset="1"/>
                                                    <GradientStop Color="#FF071923"
                                                             Offset="0"/>
                                                </LinearGradientBrush>
                                            </Ellipse.Fill>
                                        </Ellipse>
                                        <Ellipse 
                                                Width="75"
                                             Height="75"
                                             Canvas.Left="12.25"
                                             Canvas.Top="12.25"
                                             Stroke="Black"
                                             StrokeThickness="0.5"
                                             Fill="{Binding Light1, Converter={StaticResource ColorConvertValuene}}">
                                        </Ellipse>
                                    </Canvas>
                                </Viewbox>
                            </Viewbox>


                        </Grid>
                    </Border>


                    <Border Grid.Column="1" BorderThickness="5" 
                        Background="#FFD9BC81">
                        <Grid >
                            <Grid.RowDefinitions>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                            </Grid.RowDefinitions>
                            <materialDesign:Card
                                Grid.Row="0"
                                Margin="5 5 5 5"
                                Background="#FFAE9AD3"
                                Foreground="{DynamicResource PrimaryHueLightForegroundBrush}"
                                materialDesign:ShadowAssist.ShadowDepth="Depth5"
                                UniformCornerRadius="10">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid Grid.Row="0">
                                        <materialDesign:PackIcon 
                                                Kind="AccessPointNetwork" 
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center" 
                                                Width="40" Height="40"
                                                Margin="0 0 0 5"/>
                                    </Grid>
                                    <Grid Grid.Row="1">
                                        <TextBlock 
                                                FontSize="26"
                                                FontWeight="Bold"
                                                Text="{Binding Wise12, Mode=TwoWay}"
                                                HorizontalAlignment="Center"     
                                                VerticalAlignment="Center"  />
                                        <!--Text="{Binding Wise2, Mode=TwoWay}"-->
                                    </Grid>
                                </Grid>
                            </materialDesign:Card>

                            <materialDesign:Card
                                Grid.Row="1"
                                Margin="5 5 5 5"
                                Background="#FF9AC5D3"
