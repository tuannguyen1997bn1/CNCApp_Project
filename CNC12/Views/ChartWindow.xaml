        <Grid.RowDefinitions>
            <RowDefinition Height="auto"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>

        <Grid Grid.Row="0">
            <uc:ControlBar Tag="{Binding Title, ElementName=ChartWindow1}"></uc:ControlBar>
        </Grid>

