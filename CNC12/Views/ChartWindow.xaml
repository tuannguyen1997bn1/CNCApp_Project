                            HorizontalScrollBarVisibility="Auto"
                            VerticalScrollBarVisibility="Auto" >
                            <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto"></ColumnDefinition>
                                    <ColumnDefinition Width="auto"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Grid Grid.Column="0"
                                      VerticalAlignment="Stretch"
                                      HorizontalAlignment="Stretch">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="50" ></RowDefinition>
                                        <RowDefinition></RowDefinition>
                                    </Grid.RowDefinitions>
                                    <Label 
                                        Grid.Row="0"
                                            Content="{Binding ChartNameSummary1}"
                                            HorizontalContentAlignment="Center"
                                            VerticalContentAlignment="Center"
                                            FontSize="15"/>
                                    <materialDesign:Card 
                                        x:Name="cart1"
                                        Padding="10 10 10 10"
                                            Grid.Row="1"
                                            HorizontalAlignment="Left" 
                                            Height="572"
                                            Margin="10,0,10,0"                                 
                                            VerticalAlignment="Top" 
                                            Width="950" 
                                            UniformCornerRadius="20"
                                            Foreground="#DD1A2099" 
                                            FontFamily="Elephant"
                                            FontWeight="Normal">
                                        <wpf:CartesianChart  x:Name="Cart"
                                            Zoom="None" 
                                                HorizontalAlignment="Stretch" 
                                                VerticalAlignment="Stretch"  
                                                LegendLocation="Top">
                                            <wpf:CartesianChart.AxisX>
                                                <wpf:Axis 
                                                        Title="Date"   
                                                        FontSize="15" 
                                                        FontWeight="Bold" 
                                                        ShowLabels="True"  
                                                        Labels="{Binding Labels, UpdateSourceTrigger=PropertyChanged}" 
                                                        Foreground="#FF010800"/>
                                            </wpf:CartesianChart.AxisX>
                                            <wpf:CartesianChart.AxisY>
                                                <wpf:Axis 
                                                        Title="Product/Falling (Collumn - unit)"  
                                                        FontSize="15" 
                                                        FontWeight="Bold"        
                                                        LabelFormatter="{Binding Formatter}" 
                                                        Foreground="#FF002783" Position="LeftBottom" />
                                                <wpf:Axis 
                                                        Title="Total Time (Line - hour)"  
                                                        FontSize="15" 
                                                        FontWeight="Bold"  
                                                        LabelFormatter="{Binding Formatter1}" 
                                                        Position="RightTop"
                                                        Foreground="#FF738D5F"/>
                                            </wpf:CartesianChart.AxisY>
                                            <wpf:CartesianChart.Series>
                                                <wpf:StackedColumnSeries Title="Time Run"
                                                                         ScalesYAt="1"
                                                                         Values = "{Binding SummaryTotal1, UpdateSourceTrigger=PropertyChanged}">
                                                    
                                                </wpf:StackedColumnSeries>
                                                <wpf:StackedColumnSeries Title="Time Stop"
                                                                         ScalesYAt="1"
                                                                         Values = "{Binding SummaryTotal2, UpdateSourceTrigger=PropertyChanged}">

                                                </wpf:StackedColumnSeries>
                                                <wpf:StackedColumnSeries Title="Time Fail"
                                                                         ScalesYAt="1"
                                                                         Values = "{Binding SummaryTotal3, UpdateSourceTrigger=PropertyChanged}">

                                                </wpf:StackedColumnSeries>

                                                <wpf:LineSeries  
                                                        Title="Products"       
                                                        Values="{Binding SummaryProductTotal}" Stroke="#FF39E841" ScalesYAt="0">
                                                    <wpf:LineSeries.Fill>
                                                        <SolidColorBrush Color="#FF39E841" Opacity="0.15"/>
                                                    </wpf:LineSeries.Fill>

                                                </wpf:LineSeries>
                                                <wpf:LineSeries  
                                                        Title="Fails"       
                                                        Values="{Binding SummaryFallingTotal}" Stroke="#FFE55F14" ScalesYAt="0">
                                                    <wpf:LineSeries.Fill>
                                                        <SolidColorBrush Color="#FF491A03" Opacity="0.15"/>
                                                    </wpf:LineSeries.Fill>

                                                </wpf:LineSeries>
                                            </wpf:CartesianChart.Series>
                                        </wpf:CartesianChart>
                                    </materialDesign:Card>

                                </Grid>
                                <Grid Grid.Column="1" 
                                      VerticalAlignment="Stretch"
                                        HorizontalAlignment="Right">
                                    <Grid.RowDefinitions>
                                        <RowDefinition></RowDefinition>
                                        <RowDefinition></RowDefinition>
                                    </Grid.RowDefinitions>
                                    <Grid 
                                        VerticalAlignment="Stretch"
                                        HorizontalAlignment="Right"
                                        Grid.Row="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="50"></RowDefinition>
                                            <RowDefinition></RowDefinition>
                                        </Grid.RowDefinitions>

                                        <Label Grid.Row="0"
                                            Content="{Binding ChartNameSummary2}"
                                            HorizontalContentAlignment="Center"
                                            VerticalContentAlignment="Center"
                                            FontSize="15"/>
                                        <materialDesign:Card 
                                            Padding="10 10 10 10"
                                            Grid.Row="1"
                                            HorizontalAlignment="Right" 
                                            Height="238"                                 
                                            VerticalAlignment="Top" 
                                            Width="355" 
                                            UniformCornerRadius="20"
                                            Foreground="#FF27026D"
                                            FontFamily="Elephant"
                                            FontWeight="Normal">
                                            <wpf:PieChart 
                                                Zoom="Xy"
                                                ZoomingSpeed="10"
                                                Foreground="#FF3935A7"
                                                Name="pipChart1" LegendLocation="Right"       
                                                Series="{Binding SeriesCollectionProductSummary, UpdateSourceTrigger=PropertyChanged}"
                                                Hoverable="False"
                                                DataTooltip="{x:Null}" />
                                        </materialDesign:Card>
                                    </Grid>

                                    <Grid 
                                        VerticalAlignment="Bottom"
                                        HorizontalAlignment="Right"
                                        Grid.Row="1">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="50"></RowDefinition>
                                            <RowDefinition></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <Label 
                                            Grid.Row="0"
                                            Content="{Binding ChartNameSummary3}"
                                            HorizontalContentAlignment="Center"
                                            VerticalContentAlignment="Center"
                                            FontSize="15"/>
                                        <materialDesign:Card 
                                            Padding="10 10 10 10"
                                            Grid.Row="1"
                                            HorizontalAlignment="Right" 
                                            Height="238"                               
                                            VerticalAlignment="Bottom" 
                                            Width="355" 
                                            UniformCornerRadius="20"
                                            Foreground="#FF27026D"
                                            FontFamily="Elephant"
                                            FontWeight="Normal">
                                            <wpf:PieChart 
                                                Zoom="Xy"
                                                ZoomingSpeed="10"
                                                Foreground="#FF3935A7"
                                                Name="pipChart" LegendLocation="Right"       
                                                Series="{Binding SeriesCollectionFallingSummary, UpdateSourceTrigger=PropertyChanged}" 
                                                Hoverable="False"
                                                DataTooltip="{x:Null}">
                                            </wpf:PieChart>
                                        </materialDesign:Card>
                                    </Grid>
                                </Grid>
                            </Grid>
                        </ScrollViewer>
                    </materialDesign:Card>

                </Grid>
            </TabItem>
            <!--END SUMMARY-->


        </TabControl>
        <i:Interaction.Triggers>
            <i:EventTrigger EventName="SelectionChanged">
                <i:InvokeCommandAction 
                        Command="{Binding TabControlChanged}"       
                        CommandParameter="{Binding ElementName=tabcontrol}"/>
            </i:EventTrigger>
        </i:Interaction.Triggers>
    </Grid>
</Window>
